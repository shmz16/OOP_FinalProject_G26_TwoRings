            <Font size="21.0" />
         </font>
      </Label>
      <TextField fx:id="incomeTextField" layoutX="169.0" layoutY="97.0" />
      <TextField fx:id="percentageTextField" layoutX="162.0" layoutY="163.0" />
      <Button layoutX="223.0" layoutY="233.0" mnemonicParsing="false" onAction="#calculateTAxOnClickBT" text="Calculate" />
      <Label fx:id="payableTaxLable" layoutX="238.0" layoutY="288.0" textAlignment="CENTER" wrapText="true">
