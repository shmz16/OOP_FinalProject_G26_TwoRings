<AnchorPane id="AnchorPane" prefHeight="720.0" prefWidth="1080.0" style="-fx-background-color: #29333d;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.WitnessPageController">
   <children>
      <TextField fx:id="firstNameTextField" layoutX="447.0" layoutY="69.0" promptText="First Name *" style="-fx-background-color: gray; -fx-background-radius: 21; -fx-border-radius: 21;" />
      <TextField fx:id="lastNameTextField" layoutX="447.0" layoutY="113.0" promptText="Last Name *" style="-fx-background-color: gray; -fx-background-radius: 21; -fx-border-radius: 21;" />
