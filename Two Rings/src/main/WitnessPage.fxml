      <RadioButton fx:id="brideTG" layoutX="453.0" layoutY="332.0" mnemonicParsing="false" selected="true" style="-fx-background-color: #0c84ff; -fx-background-radius: 21;" text="Bride">
         <toggleGroup>
            <ToggleGroup fx:id="gg" />
         </toggleGroup>
      </RadioButton>
      <RadioButton fx:id="groomTG" layoutX="553.0" layoutY="332.0" mnemonicParsing="false" style="-fx-background-color: #0c84ff; -fx-background-radius: 21; -fx-border-radius: 21;" text="Groom" toggleGroup="$gg" />
