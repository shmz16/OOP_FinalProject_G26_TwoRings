      <TextField fx:id="passwordTextField" layoutX="717.0" layoutY="69.0" promptText="Password *" style="-fx-background-color: gray; -fx-background-radius: 21; -fx-border-radius: 21;" />
      <TextField fx:id="comfirmPasswordTextField" layoutX="717.0" layoutY="113.0" promptText="Confirm Password *" style="-fx-background-color: gray; -fx-background-radius: 21; -fx-border-radius: 21;" />
      <TextField fx:id="phoneTextField" layoutX="717.0" layoutY="155.0" promptText="Phone *" />
